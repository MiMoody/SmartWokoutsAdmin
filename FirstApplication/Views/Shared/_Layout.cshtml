<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - приложение ASP.NET</title>
    @Styles.Render("~/Content/Header.css")
</head>
<body>

    <nav>
        <ul class="menu">
            <li>@Html.ActionLink("Новости", "Index", "Posts")</li>
            <li>@Html.ActionLink("Упражнения", "Index", "Exercises")</li>
            <li>@Html.ActionLink("Подписки", "Index", "Premium_Works")</li>
            <li>@Html.ActionLink("Тренировки", "Index", "Workouts")</li>
        </ul>
    </nav>


    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("https://kit.fontawesome.com/b781c44b15.js")
    @Scripts.Render("~/scripts/jquery-3.5.1.min.js")
    @Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")

    @RenderSection("scripts", required: false)


</body>
</html>
